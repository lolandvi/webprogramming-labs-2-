def if16():
    a = 3
    b = 4
    c = 5
    if a < b < c:
        a = a*2
        b = b*2
        c = c*2
    else:
        a = a*-1
        b = b*-1
        c = b*-1
    print(a, b, c)
        
def for1():
    n = 3
    k = 3
    if n > 0:
        for i in range(k):
            print(n)
        
def for36(n, k):
    sum = 0
    if n > 0:
        for i in range(n+1):
            sum += i**k
    print(sum)

if16()
for1()
for36(3, 3)
